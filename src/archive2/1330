import java.util.Scanner;

public class Main {
	    public static void main(String[] args) {
	    	Scanner scn = new Scanner(System.in);
	    	int a, b;
	    	a = scn.nextInt();
	    	b = scn.nextInt();
	    	if (a > b) {
	    		System.out.println(">");
	    	}else if (a < b) {
	    		System.out.println("<");
	    	}else {
	    		System.out.println("==");
	    	}
	    	
	  

	    }
}
